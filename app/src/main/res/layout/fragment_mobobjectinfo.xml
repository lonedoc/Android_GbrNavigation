<?xml version="1.0" encoding="utf-8"?>
    <ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        tools:context="mobVersion.login.OldLoginActivity"
        android:id="@+id/linearLayout"
        android:fillViewport="true"
        android:background="@color/textWhite">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@color/textWhite"
            android:orientation="vertical">

            <TextView
                android:id="@+id/mob_textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="@string/object_name"
                android:textColor="@color/textDark"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/mob_card_object_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:textColor="@color/textDark"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/mob_textView2"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/mob_textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="@string/object_address"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_name" />

            <TextView
                android:id="@+id/mob_card_object_address"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/mob_textView3"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_name" />

            <TextView
                android:id="@+id/mob_textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="@string/object_number"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_address" />

            <TextView
                android:id="@+id/mob_card_object_number"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/mob_textView11"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_address" />

            <TextView
                android:id="@+id/mob_textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="@string/object_customer"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_number" />

            <TextView
                android:id="@+id/mob_card_object_customer"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/mob_textView4"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_number" />

            <TextView
                android:id="@+id/mob_textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="@string/object_inn"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_customer" />

            <TextView
                android:id="@+id/mob_card_object_inn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/mob_textView5"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_customer" />

            <TextView
                android:id="@+id/mob_textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="@string/object_alarm"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_inn" />

            <TextView
                android:id="@+id/mob_card_object_alarm"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/mob_textView6"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_inn" />

            <TextView
                android:id="@+id/mob_textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginStart="8dp"
                android:text="@string/object_alarm_time"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_alarm" />

            <TextView
                android:id="@+id/mob_card_object_alarm_time"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/mob_textView7"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_alarm" />

            <TextView
                android:id="@+id/mob_textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="@string/object_alarm_apply_time"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_alarm_time" />

            <TextView
                android:id="@+id/mob_card_object_alarm_apply_time"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/mob_textView8"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_alarm_time" />

            <TextView
                android:id="@+id/mob_textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="@string/object_time_arrived"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_alarm_apply_time" />

            <TextView
                android:id="@+id/mob_card_object_time_arrived"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/mob_textView9"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_alarm_apply_time" />

            <TextView
                android:id="@+id/mob_textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="@string/object_additionally"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_time_arrived" />

            <TextView
                android:id="@+id/mob_card_object_additionally"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:textColor="@color/textDark"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mob_textView10" />

            <Button
                android:id="@+id/mob_card_send_arrived"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="16dp"
                android:backgroundTint="@color/button_reports_send"
                android:enabled="false"
                android:gravity="center"
                android:text="@string/sendArrived"
                android:textColor="@color/text_button_report_send"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_object_additionally" />

            <Button
                android:id="@+id/mob_card_send_reports"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:backgroundTint="@color/button_reports_send"
                android:enabled="false"
                android:gravity="center"
                android:layout_marginTop="8dp"
                android:text="@string/sendReports"
                android:textColor="@color/text_button_report_send"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="@+id/mob_card_send_arrived"
                app:layout_constraintStart_toStartOf="@+id/mob_card_send_arrived"
                app:layout_constraintTop_toBottomOf="@+id/mob_card_send_arrived" />

        </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>